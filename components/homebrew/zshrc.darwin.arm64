#!/usr/bin/env zsh
(( ${path[(I)/opt/homebrew/bin]} )) || path=( /opt/homebrew/bin $path )
(( ${path[(I)/opt/homebrew/share/zsh/site-functions]} )) || fpath=( /opt/homebrew/share/zsh/site-functions $fpath )
(( ${path[(I)/opt/homebrew/share/zsh-completions]} )) || fpath=( /opt/homebrew/share/zsh-completions $fpath )

  export PATH
  export FPATH
